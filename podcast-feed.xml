---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:itunes="http://www.itunes.com/dtds/podcast-1.0.dtd" xmlns:podcast="https://github.com/Podcastindex-org/podcast-namespace/blob/main/docs/1.0.md">
	<channel>
		<title>{{ site.title }}</title>
		<description>Siblings celebrating and borderline obsessing over their favorite records.</description>
		<link>{{ site.url }}</link>
		<language>en-US</language>
		<generator uri="https://jekyllrb.com/" version="{{ jekyll.version }}">Jekyll</generator>
		<pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
		<lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
		<managingEditor>badmusichertz@gmail.com (Marc Barrowclift)</managingEditor>
		<webMaster>badmusichertz@gmail.com (Marc Barrowclift)</webMaster>
		<image>
			<url>{{ site.url }}/images/logo/podcast-cover-art.jpg</url>
			<title>{{ site.title }}</title>
			<link>{{ site.url }}</link>
		</image>
		<copyright>© 2017-{{ site.time | date: "%Y" }} Marc Barrowclift</copyright>

		<podcast:guid>7cb2e396-f4e2-5d79-a7a6-584baa119f1e</podcast:guid>
		<podcast:medium>podcast</podcast:medium>
		<podcast:images src="{{ site.url }}/images/logo/podcast-cover-art.jpg" srcset="{{ site.url }}/images/logo/podcast-cover-art.jpg x1" />

		<itunes:author>The Barrowclift Sibs</itunes:author>
		<itunes:explicit>no</itunes:explicit>
		<itunes:type>episodic</itunes:type>
		<itunes:category text="Music">
			<itunes:category text="Music Commentary"/>
		</itunes:category>
		<itunes:owner>
			<itunes:name>The Barrowclift Sibs</itunes:name>
			<itunes:email>badmusichertz@gmail.com</itunes:email>
		</itunes:owner>
		<itunes:image href="{{ site.url }}/images/logo/podcast-cover-art.jpg" />
		<itunes:copyright>© 2017-{{ site.time | date: "%Y" }} Marc Barrowclift</itunes:copyright>
		<itunes:summary>Siblings celebrating and borderline obsessing over their favorite records.</itunes:summary>
		<itunes:subtitle>Siblings celebrating and borderline obsessing over their favorite records.</itunes:subtitle>

		{% for post in site.posts %}
		{% if post.layout == 'podcast' or post.layout == 'podcast-v2' %}
		<item>
			<title>{{ post.episode }}. {{ post.title | xml_escape }}</title>
			<description><![CDATA[{{ post.content | cdata_escape }}]]></description>
			<link>{{ post.url | prepend: site.url }}</link>
			<guid isPermaLink="true">{{ site.dropbox-baseurl | xml_escape }}/{{ post.album-name | xml_escape }}/{{ post.podcast-filename | xml_escape }}</guid>
			<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
			<author>The Barrowclift Sibs</author>
			<enclosure url="{{ site.dropbox-baseurl | xml_escape }}/{{ post.album-name | xml_escape }}/{{ post.podcast-filename | xml_escape }}" length="{{ post.podcast-size-in-bytes | xml_escape }}" type="audio/mpeg"/>

			<podcast:images src="{{ site.url }}/images/logo/podcast-cover-art.jpg" srcset="{{ site.dropbox-baseurl | xml_escape }}/{{ post.album-name | xml_escape }}/images/podcast-art.jpg x1" />
			<podcast:episode>{{ post.episode }}</podcast:episode>

			<itunes:image href="{{ site.dropbox-baseurl | xml_escape }}/{{ post.album-name | xml_escape }}/images/podcast-art.jpg" />
			<itunes:explicit>{% if post.podcast-explicit %}yes{% else %}no{% endif %}</itunes:explicit>
			<itunes:episodeType>full</itunes:episodeType>
			<itunes:title>{{ post.title | xml_escape }}</itunes:title>
			<itunes:author>The Barrowclift Sibs</itunes:author>
			<itunes:episode>{{ post.episode }}</itunes:episode>
			<itunes:duration>{{ post.podcast-duration | xml_escape }}</itunes:duration>
			<itunes:subtitle>{{ post.excerpt }}</itunes:subtitle>
		</item>
		{% endif %}
		{% endfor %}
	</channel>
</rss>
